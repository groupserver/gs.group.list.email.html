<tal:block xmlns:tal="http://xml.zope.org/namespaces/tal">
/*gs-group-list-email-html CSS*/
#gs-group-list-email-html-prologue-metadata td {
    padding: 0 0 0 10px;
    margin: 0;
}
#gs-group-list-email-html-prologue-metadata-group h2 {
    padding:0;
    margin: 0;
}
#gs-group-list-email-html hr {
    padding: 9px 0 0 0;
    margin: 0;
    border-top: none;
    border-bottom-width: 1px;
    border-bottom-color: #eeeeee;
}
#gs-group-list-email-html-body-main {
    padding: 10px 0 0 0;
    margin: 0;
}
#gs-group-list-email-html-body .line {
    display: inline-block;
    white-space: pre-wrap;
    width: 100%;
    max-width: 580px;  /* 600px - padding*/
    overflow: hidden;
    text-overflow: ellipsis;
}
#gs-group-list-email-html-body .line a {
    text-decoration: underline;
}

#gs-group-list-email-html-body-read-more td {
    padding: 10px 0 0 0;
    margin: 0;
}
#gs-group-list-email-html-body,
#gs-group-list-email-html-links td {
    padding: 20px 0 0 0;
    margin: 0;
}
#gs-group-list-email-html-files h4 {
    padding: 10px 0 0 0;
    margin: 0;
}
.gs-group-list-email-html-files-file {
    padding: 10px 0 0 0;
    margin: 0;
}
#gs-group-list-email-html-links-view-post {
    display: inline-block;
    line-height: 30px;
}
</tal:block>
