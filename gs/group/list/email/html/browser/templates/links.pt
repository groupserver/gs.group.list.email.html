<table id="gs-group-list-email-html-links"
       xmlns:tal="http://xml.zope.org/namespaces/tal"
       align="center" valign="center"
       border="0" cellpadding="0" cellspacing="0" width="100%">
  <tr>
    <td><hr/></td>
  </tr>
  <tr id="gs-group-list-email-html-links-view">
    <td>
      <tal:block condition="view/replyLink">
        <a id="gs-group-list-email-html-links-view-reply"
           href="#" title="Reply to this email" class="btn"
           tal:attributes="href view/replyLink">&#11178; Reply</a>
        &#160;
      </tal:block>
      <tal:block condition="view/newLink">
        <a id="gs-group-list-email-html-links-view-new"
           href="#" title="Start a new topic by email" class="btn"
           tal:attributes="href view/newLink">&#128386; New topic</a>
        &#160;
      </tal:block>
      <a id="gs-group-list-email-html-links-view-topic" class="btn"
         href="#" title="View this post within the topic on the web"
         tal:attributes="href view/post/topicURL">View topic&#8230;</a>
    </td><!--gs-group-list-email-html-links-view-topic-->
  </tr>
  <tr id="gs-group-list-email-html-links-settings">
    <td>
      <a tal:attributes="href view/leaveLink"
         href="#">Unsubscribe</a>
      <span class="muted">&#160;&#8226;&#160;</span>
      <a tal:attributes="href view/digestLink"
         href="#" >Switch to a daily digest</a>
    </td>
  </tr>
</table><!--gs-group-list-email-html-links-->
