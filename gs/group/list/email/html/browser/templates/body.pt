<table id="gs-group-list-email-html-body"
       xmlns:tal="http://xml.zope.org/namespaces/tal"
       align="left" valign="center"
       border="0" cellpadding="0" cellspacing="0" width="100%">
  <tr>
    <td><hr/></td>
  </tr>
  <tr>
    <td id="gs-group-list-email-html-body-main" align="left"
        tal:content="structure view/htmlBody">
      The body of the post.
    </td>
  </tr>
  <tr id="gs-group-list-email-html-body-read-more"
      tal:condition="view/readMore">
    <td>
      <a tal:attributes="href string:${view/post/url}"
         class="small muted"
         href="#">Rest of post&#8230;</a>
    </td>
  </tr>
</table>
